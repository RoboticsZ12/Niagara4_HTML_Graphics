                                                                  <!-- Hamburger Menu Script -->
                                                                            <script>
const hamburgerMenu = document.getElementById('hamburgerMenu');
const sidePanel = document.getElementById('sidePanel');
const overlay = document.getElementById('overlay');
const ahuDropdown = document.getElementById('ahuDropdown');
const ahuOptions = document.querySelectorAll('.ahu-option');

// Toggle side panel
hamburgerMenu.addEventListener('click', function() {
hamburgerMenu.classList.toggle('active');
sidePanel.classList.toggle('active');
overlay.classList.toggle('active');
});

// Close side panel when clicking overlay
overlay.addEventListener('click', function() {
hamburgerMenu.classList.remove('active');
sidePanel.classList.remove('active');
overlay.classList.remove('active');
});

// Toggle dropdown
ahuDropdown.querySelector('.ahu-dropdown-header').addEventListener('click', function() {
ahuDropdown.classList.toggle('active');
});

// Handle AHU selection
ahuOptions.forEach(option => {
option.addEventListener('click', function() {
// Remove active class from all options
ahuOptions.forEach(opt => opt.classList.remove('active'));

// Add active class to clicked option
this.classList.add('active');

// Update the dropdown header text
const selectedAhu = this.textContent;
ahuDropdown.querySelector('h3').textContent = selectedAhu;

// Close dropdown
ahuDropdown.classList.remove('active');

// Here you can add logic to switch between AHU data
console.log('Selected AHU:', this.getAttribute('data-ahu'));

// Optional: Close side panel after selection
setTimeout(() => {
hamburgerMenu.classList.remove('active');
sidePanel.classList.remove('active');
overlay.classList.remove('active');
}, 300);
});
});

// Close dropdown when clicking outside
document.addEventListener('click', function(e) {
if (!ahuDropdown.contains(e.target)) {
ahuDropdown.classList.remove('active');
}
});
 </script>

    


<!-- COMBINED NUMERIC AND BOOLEAN OVERRIDE - FIXED VERSION -->
<script>
require(['baja!', 'baja!control:Override', 'baja!control:NumericOverride'], function (baja, types) {
  'use strict';

  
// const headerName = document.querySelector('.ahu-title').innerText.trim();
// const pathName = headerName.replace(/-/g, '_');

        // Get filename from URL and extract the name part
        const fullFilename = window.location.pathname.split('/').pop();
        const headerName = fullFilename.replace('.html', ''); // Remove .html extension
        const pathName = headerName.replace(/-/g, '_'); //add underscores

        document.getElementById('ahu-header').textContent = pathName;


function makePath(pointName) {
return `station:|slot:/Drivers/BcpBacnetNetwork/${pathName}/points/${pointName}`;
}

const numericPoints = {
Air_Flow_SP: makePath('ActFlowSP'),
Damper_CMD: makePath('DamperCommand'),
Effective_Space_TMP: makePath('EffectSpaceTemp'),
Unocc_CoolSP: makePath('UnoccCoolSP'),
Occ_CoolSP: makePath('OccCoolSP'),
Occ_HeatSP: makePath('OccHeatSP'),
Unocc_HeatSP: makePath('UnoccHeatSP'),
Cool_SP: makePath('ActCoolSP'),
Heat_SP: makePath('ActHeatSP'),
};
  

// const headerName2 = document.querySelector('.ahu-title').innerText.trim();
// const pathName2 = headerName2.replace(/-/g, '_');
        // Get filename from URL and extract the name part
        const fullFilename2 = window.location.pathname.split('/').pop();
        const headerName2 = fullFilename2.replace('.html', ''); // Remove .html extension
        const vavSlotName2 = headerName2.replace(/-/g, '_');


function makePath(pointName) {
return `station:|slot:/Drivers/BcpBacnetNetwork/${pathName}/points/${pointName}`;
}

const booleanPoints = {
Coil_Reheat: makePath('Reheat1'),
UnitStatus: makePath('UnitStatus'),
};




  // Separate resolved points storage
  let resolvedNumericPoints = {};
  let resolvedBooleanPoints = {};
  let currentActiveNumericPoint = null;
  let currentActiveBooleanPoint = null;

  // Get DOM elements
  const numericInput = document.getElementById('numeric-input');
  const numericDurationDropdown = document.getElementById('numeric-duration-dropdown');
  const numericOverrideBtn = document.getElementById('numeric-override-btn');
  const numericReleaseBtn = document.getElementById('numeric-release-btn');
  const numericPointSelector = document.getElementById('numeric-point-selector');
  const numericPointName = document.getElementById('numeric-point-name');
  const numericPointValue = document.getElementById('numeric-point-value');

  const commandDropdown = document.getElementById('command-dropdown');
  const booleanDurationDropdown = document.getElementById('boolean-duration-dropdown');
  const booleanOverrideBtn = document.getElementById('boolean-override-btn');
  const booleanReleaseBtn = document.getElementById('boolean-release-btn');
  const booleanPointSelector = document.getElementById('boolean-point-selector');
  const booleanPointName = document.getElementById('boolean-point-name');
  const booleanPointValue = document.getElementById('boolean-point-value');

  // Resolve all points
  async function resolveAllPoints() {
    // Resolve numeric points
    const numericPromises = Object.entries(numericPoints).map(async ([pointName, pointOrd]) => {
      try {
        const point = await baja.Ord.make(pointOrd).get();
        resolvedNumericPoints[pointName] = point;
        console.log(`Numeric point ${pointName} resolved`, point);
        debugPointActions(point, pointName);
        return { pointName, success: true, type: 'numeric' };
      } catch (err) {
        console.error(`Failed to resolve numeric point ${pointName}`, err);
        return { pointName, success: false, type: 'numeric' };
      }
    });

    // Resolve boolean points
    const booleanPromises = Object.entries(booleanPoints).map(async ([pointName, pointOrd]) => {
      try {
        const point = await baja.Ord.make(pointOrd).get();
        resolvedBooleanPoints[pointName] = point;
        console.log(`Boolean point ${pointName} resolved`, point);
        debugPointActions(point, pointName);
        return { pointName, success: true, type: 'boolean' };
      } catch (err) {
        console.error(`Failed to resolve boolean point ${pointName}`, err);
        return { pointName, success: false, type: 'boolean' };
      }
    });

    await Promise.all([...numericPromises, ...booleanPromises]);
    initializeButtonListeners();
  }

  function debugPointActions(point, pointName) {
    console.log(`=== ACTIONS FOR ${pointName} ===`);
    
    try {
      if (point.getType && typeof point.getType === 'function') {
        const type = point.getType();
        console.log('Point type:', type);
        
        if (type.getActions) {
          const actions = type.getActions();
          console.log('Available actions:', actions);
          
          if (actions && actions.list) {
            actions.list().forEach(action => {
              console.log(`- Action: ${action.getName()}`);
            });
          }
        }
      }

      if (point.getSlots) {
        const slots = point.getSlots();
        console.log('Available slots:', slots);
      }

      const commonActions = ['override', 'set', 'setValue', 'emergencyOverride', 'manualOverride', 'active', 'inactive', 'auto'];
      commonActions.forEach(actionName => {
        try {
          if (point.getAction && point.getAction(actionName)) {
            console.log(`âœ“ Found action: ${actionName}`);
          }
        } catch (e) {
          // Action doesn't exist, that's ok
        }
      });

    } catch (error) {
      console.error('Error discovering actions:', error);
    }
    
    console.log('=== END ACTIONS DEBUG ===');
  }

  resolveAllPoints();

  function initializeButtonListeners() {
    // Initialize numeric point selector
    if (numericPointSelector) {
      numericPointSelector.innerHTML = '';
      Object.keys(resolvedNumericPoints).forEach(pointName => {
        const option = document.createElement('option');
        option.value = pointName;
        option.textContent = pointName;
        numericPointSelector.appendChild(option);
      });

      if (Object.keys(resolvedNumericPoints).length > 0) {
        currentActiveNumericPoint = Object.keys(resolvedNumericPoints)[0];
        numericPointSelector.value = currentActiveNumericPoint;
        updateNumericPointDisplay();
      }

      numericPointSelector.addEventListener('change', function () {
        currentActiveNumericPoint = this.value;
        console.log('Active numeric point changed to:', currentActiveNumericPoint);
        updateNumericPointDisplay();
      });
    } else {
      currentActiveNumericPoint = Object.keys(resolvedNumericPoints)[0];
      if (currentActiveNumericPoint) {
        updateNumericPointDisplay();
      }
    }

    // Initialize boolean point selector
    if (booleanPointSelector) {
      booleanPointSelector.innerHTML = '';
      Object.keys(resolvedBooleanPoints).forEach(pointName => {
        const option = document.createElement('option');
        option.value = pointName;
        option.textContent = pointName;
        booleanPointSelector.appendChild(option);
      });

      if (Object.keys(resolvedBooleanPoints).length > 0) {
        currentActiveBooleanPoint = Object.keys(resolvedBooleanPoints)[0];
        booleanPointSelector.value = currentActiveBooleanPoint;
        updateBooleanPointDisplay();
      }

      booleanPointSelector.addEventListener('change', function () {
        currentActiveBooleanPoint = this.value;
        console.log('Active boolean point changed to:', currentActiveBooleanPoint);
        updateBooleanPointDisplay();
      });
    } else {
      currentActiveBooleanPoint = Object.keys(resolvedBooleanPoints)[0];
      if (currentActiveBooleanPoint) {
        updateBooleanPointDisplay();
      }
    }

    // NUMERIC OVERRIDE BUTTON
    if (numericOverrideBtn) {
      numericOverrideBtn.addEventListener('click', () => {
        const activePoint = getActiveNumericPoint();
        if (!activePoint) return;

        const value = parseFloat(numericInput.value);
        if (isNaN(value)) {
          alert('Please enter a valid number.');
          return;
        }

        const duration = numericDurationDropdown ? numericDurationDropdown.value : null;
        let durationSeconds = null;

        if (duration) {
          switch (duration) {
            case '1hour': durationSeconds = 3600; break;
            case '2hours': durationSeconds = 7200; break;
            case '4hours': durationSeconds = 14400; break;
            case '8hours': durationSeconds = 28800; break;
          }
        }

        performNumericOverride(activePoint, value, durationSeconds);
      });
    }

    // NUMERIC RELEASE BUTTON
    if (numericReleaseBtn) {
      numericReleaseBtn.addEventListener('click', () => {
        const activePoint = getActiveNumericPoint();
        if (activePoint) {
          releaseToAuto(activePoint);
        }
      });
    }

    // BOOLEAN OVERRIDE BUTTON
    if (booleanOverrideBtn) {
      booleanOverrideBtn.addEventListener('click', function() {
        const activePoint = getActiveBooleanPoint();
        if (!activePoint) {
          console.error('No active boolean point available');
          return;
        }
        
        const command = commandDropdown.value;
        const duration = booleanDurationDropdown ? booleanDurationDropdown.value : null;
        
        console.log('Boolean override button clicked - Point:', currentActiveBooleanPoint, 'Command:', command, 'Duration:', duration);
        
        let durationSeconds = null;
        if (duration && duration !== 'permanent') {
          switch(duration) {
            case '1hour': durationSeconds = 3600; break;
            case '2hours': durationSeconds = 7200; break;
            case '4hours': durationSeconds = 14400; break;
            case '8hours': durationSeconds = 28800; break;
          }
        }
        
        if (command === 'on') {
          performActiveOverride(activePoint, durationSeconds);
        } else if (command === 'off') {
          performInactiveOverride(activePoint, durationSeconds);
        } else if (command === 'auto') {
          releaseToAuto(activePoint);
        }
      });
    }

    // BOOLEAN RELEASE BUTTON
    if (booleanReleaseBtn) {
      booleanReleaseBtn.addEventListener('click', function() {
        const activePoint = getActiveBooleanPoint();
        if (!activePoint) {
          console.error('No active boolean point available');
          return;
        }
        
        console.log('Boolean release button clicked for point:', currentActiveBooleanPoint);
        releaseToAuto(activePoint);
      });
    }
  }

  function getActiveNumericPoint() {
    if (!currentActiveNumericPoint || !resolvedNumericPoints[currentActiveNumericPoint]) {
      console.error('Active numeric point not resolved:', currentActiveNumericPoint);
      return null;
    }
    return resolvedNumericPoints[currentActiveNumericPoint];
  }

  function getActiveBooleanPoint() {
    if (!currentActiveBooleanPoint || !resolvedBooleanPoints[currentActiveBooleanPoint]) {
      console.error('Active boolean point not resolved:', currentActiveBooleanPoint);
      return null;
    }
    return resolvedBooleanPoints[currentActiveBooleanPoint];
  }

  function performNumericOverride(point, value, durationSeconds = null) {
    console.log('Performing numeric override with value:', value);

    try {
      const overrideParam = baja.$('control:NumericOverride');
      overrideParam.setValue(value);
      
      if (durationSeconds !== null) {
        overrideParam.setDuration(baja.RelTime.make({ seconds: durationSeconds }));
        console.log(`Timed numeric override for ${durationSeconds} seconds`);
      } else {
        console.log('Permanent numeric override');
      }
      
      point.invoke({ 
          slot: 'override', 
          value: overrideParam 
      })
      .then(() => {
        console.log('âœ“ Numeric override successful');
        console.log('New value:', point.getOut().getValueDisplay());
        // Update display immediately after override
        setTimeout(updateNumericPointDisplay, 500);
      })
      .catch(error => {
        console.log('âœ— Numeric override failed:', error.message);
      });
      
    } catch (error) {
      console.error('Error creating numeric override parameter:', error);
    }
  }

  function performActiveOverride(point, durationSeconds = null) {
    console.log('Performing ACTIVE override...');
    
    let overrideParam = baja.$("control:Override");
    
    if (durationSeconds !== null) {
      overrideParam.setDuration(baja.RelTime.make({ seconds: durationSeconds }));
      console.log(`Setting timed active override for ${durationSeconds} seconds`);
    } else {
      console.log('Setting permanent active override');
    }
    
    point.invoke({ 
      slot: 'active', 
      value: overrideParam 
    })
    .then(function() {
      console.log('ACTIVE override successful');
      console.log('Current value:', point.getOut().getValueDisplay());
      // Update display immediately after override
      setTimeout(updateBooleanPointDisplay, 500);
    })
    .catch(function(error) {
      console.error('ACTIVE override failed:', error);
    });
  }

  // Function to update numeric point display
  function updateNumericPointDisplay() {
    if (!currentActiveNumericPoint || !resolvedNumericPoints[currentActiveNumericPoint]) {
      if (numericPointName) numericPointName.textContent = 'No Point Selected';
      if (numericPointValue) numericPointValue.textContent = 'N/A';
      return;
    }

    const point = resolvedNumericPoints[currentActiveNumericPoint];
    
    // Update point name
   if (numericPointName) {
  const status = point.getStatus();
  if (status && status.toString().toLowerCase().includes('override')) {
    numericPointName.innerHTML = `<span class="point-name-override">${currentActiveNumericPoint} (Overridden)</span>`;
  } else {
    numericPointName.textContent = currentActiveNumericPoint;
  }
}

    
    // Update point value
    if (numericPointValue) {
      try {
        const currentValue = point.getOut().getValueDisplay();
        const status = point.getStatus();
        numericPointValue.textContent = currentValue;
        
        // Update CSS class based on status
        numericPointValue.className = 'data-value';
        if (status && status.toString().toLowerCase().includes('override')) {
          numericPointValue.classList.add('status-override');
        } else if (status && status.toString().toLowerCase().includes('fault')) {
          numericPointValue.classList.add('status-fault');
        } else {
          numericPointValue.classList.add('status-on');
        }
      } catch (error) {
        numericPointValue.textContent = 'Error reading value';
        numericPointValue.className = 'data-value status-fault';
        console.error('Error reading numeric point value:', error);
      }
    }
  }

  // Function to update boolean point display
  function updateBooleanPointDisplay() {
    if (!currentActiveBooleanPoint || !resolvedBooleanPoints[currentActiveBooleanPoint]) {
      if (booleanPointName) booleanPointName.textContent = 'No Point Selected';
      if (booleanPointValue) booleanPointValue.textContent = 'N/A';
      return;
    }

    const point = resolvedBooleanPoints[currentActiveBooleanPoint];
    
    // Update point name
    if (booleanPointName) {
      booleanPointName.textContent = currentActiveBooleanPoint;
    }
    
    // Update point value
    if (booleanPointValue) {
      try {
        const currentValue = point.getOut().getValueDisplay();
        const status = point.getStatus();
        booleanPointValue.textContent = currentValue;
        
        // Update CSS class based on status
        booleanPointValue.className = 'data-value';
        if (status && status.toString().toLowerCase().includes('override')) {
          booleanPointValue.classList.add('status-override');
        } else if (status && status.toString().toLowerCase().includes('fault')) {
          booleanPointValue.classList.add('status-fault');
        } else if (currentValue && currentValue.toString().toLowerCase() === 'true') {
          // Special handling for RoomOccupancy and UnitStatus - flip the colors
          if (currentActiveBooleanPoint === 'Room_Occupancy' || currentActiveBooleanPoint === 'UnitStatus') {
            booleanPointValue.classList.add('status-off'); // Green when true
          } else {
            booleanPointValue.classList.add('status-on'); // Red when true
          }
        } else {
          // Special handling for RoomOccupancy and UnitStatus - flip the colors
          if (currentActiveBooleanPoint === 'Room_Occupancy' || currentActiveBooleanPoint === 'UnitStatus') {
            booleanPointValue.classList.add('status-on'); // Red when false
          } else {
            booleanPointValue.classList.add('status-off'); // Green when false
          }
        }
      } catch (error) {
        booleanPointValue.textContent = 'Error reading value';
        booleanPointValue.className = 'data-value status-fault';
        console.error('Error reading boolean point value:', error);
      }
    }
  }

  // Function to start real-time updates
  function startRealTimeUpdates() {
    setInterval(() => {
      if (currentActiveNumericPoint && resolvedNumericPoints[currentActiveNumericPoint]) {
        updateNumericPointDisplay();
      }
      if (currentActiveBooleanPoint && resolvedBooleanPoints[currentActiveBooleanPoint]) {
        updateBooleanPointDisplay();
      }
    }, 2000); // Update every 2 seconds
  }

  // Start real-time updates after initialization
  setTimeout(startRealTimeUpdates, 1000);

  function performInactiveOverride(point, durationSeconds = null) {
    console.log('Performing INACTIVE override...');
    
    let overrideParam = baja.$("control:Override");
    
    if (durationSeconds !== null) {
      overrideParam.setDuration(baja.RelTime.make({ seconds: durationSeconds }));
      console.log(`Setting timed inactive override for ${durationSeconds} seconds`);
    } else {
      console.log('Setting permanent inactive override');
    }
    
    point.invoke({ 
      slot: 'inactive', 
      value: overrideParam 
    })
    .then(function() {
      console.log('INACTIVE override successful');
      console.log('Current value:', point.getOut().getValueDisplay());
      // Update display immediately after override
      setTimeout(updateBooleanPointDisplay, 500);
    })
    .catch(function(error) {
      console.error('INACTIVE override failed:', error);
    });
  }

  function releaseToAuto(point) {
    console.log('Releasing to auto');
    
    point.invoke({ slot: 'auto' })
      .then(() => {
        console.log('Released to AUTO successfully');
        console.log('Current value:', point.getOut().getValueDisplay());
        // Update displays immediately after release
        setTimeout(() => {
          updateNumericPointDisplay();
          updateBooleanPointDisplay();
        }, 500);
      })
      .catch(error => {
        console.error('Release to AUTO failed:', error);
      });
  }

  // Global helper functions
  window.overrideNumericPointByName = (pointName, value, durationSeconds = null) => {
    const point = resolvedNumericPoints[pointName];
    if (!point) {
      console.error(`Numeric point ${pointName} not found`);
      return;
    }
    performNumericOverride(point, value, durationSeconds);
  };

  window.overrideBooleanPointByName = (pointName, command, durationSeconds = null) => {
    const point = resolvedBooleanPoints[pointName];
    if (!point) {
      console.error(`Boolean point ${pointName} not found`);
      return;
    }
    
    if (command === 'on') {
      performActiveOverride(point, durationSeconds);
    } else if (command === 'off') {
      performInactiveOverride(point, durationSeconds);
    } else if (command === 'auto') {
      releaseToAuto(point);
    }
  };

  window.releaseAllPointsToAuto = () => {
    Object.entries(resolvedNumericPoints).forEach(([pointName, point]) => {
      console.log(`Releasing numeric ${pointName} to AUTO...`);
      releaseToAuto(point);
    });
    
    Object.entries(resolvedBooleanPoints).forEach(([pointName, point]) => {
      console.log(`Releasing boolean ${pointName} to AUTO...`);
      releaseToAuto(point);
    });
  };

  // Debug helpers
  window.debugNumericPoint = (pointName) => {
    const point = resolvedNumericPoints[pointName];
    if (!point) {
      console.log('Numeric point not found:', pointName);
      return;
    }
    debugPointActions(point, pointName);
  };

  window.debugBooleanPoint = (pointName) => {
    const point = resolvedBooleanPoints[pointName];
    if (!point) {
      console.log('Boolean point not found:', pointName);
      return;
    }
    debugPointActions(point, pointName);
  };

  // Expose resolved points for debugging
  window.resolvedNumericPoints = resolvedNumericPoints;
  window.resolvedBooleanPoints = resolvedBooleanPoints;
});
</script>




 <!-- AHU Data Monitoring Script -->
  <script>
        require(['baja!'], function (baja) {
            'use strict';


  // const headerName = document.querySelector('.ahu-title').innerText.trim();
  // const vavSlotName = headerName.replace(/-/g, '_');
          // Get filename from URL and extract the name part
        const fullFilename = window.location.pathname.split('/').pop();
        const headerName = fullFilename.replace('.html', ''); // Remove .html extension
        const vavSlotName = headerName.replace(/-/g, '_');
  
  

  // Build full slot path for a given point under the VAV
  function vavPath(pointName) {
    return `station:|slot:/Drivers/BcpBacnetNetwork/${vavSlotName}/points/${pointName}`;
  }

  // ADD POINTS HERE!!!!!!!!!!!!!

  const dataPoints = {
    ReheatTrack: vavPath('ReheatTrack'),
    Reheat1: vavPath('Reheat1'),
    HotWaterReheat: vavPath('HotWaterReheat'),
    UnitStat: vavPath('UnitStatus'),
    OccupancyCMD: vavPath('RoomOccupancy'),
    DischargeAirTMP: vavPath('DischargeTemp'),
    SpaceTemp: vavPath('SpaceTemp'),
    DPRCMD: vavPath('DamperCommand'),
    DPRPOS: vavPath('DamperPosition'),
    VAVDIFFPRESS: vavPath('VAVDifferentialPressure'),
    AirFlow: vavPath('ActFlow'),
    AirFlowSP: vavPath('ActFlowSP'),
    OutsideAir: vavPath('OutdoorTemp'),
    ATIVECOOLSP: vavPath('ActCoolSP'),
    ATIVEHEATSP: vavPath('ActHeatSP'),
    UnoccupiedCoolSP: vavPath('UnoccCoolSP'),
    OccupiedCoolSP: vavPath('OccCoolSP'),
    OccupiedHeatSP: vavPath('OccHeatSP'),
    UnoccupiedHeatSP: vavPath('UnoccHeatSP'),

  };


            
            // HTML elements to update
            const elements = {
                ReheatTrack: document.getElementById('ReheatTrack'),
                Reheat1: document.getElementById('Reheat1'),
                HotWaterReheat: document.getElementById('Re-heat'),
                UnitStat: document.getElementById('Un-Stat'),
                OccupancyCMD: document.getElementById('Occupancy'),
                DischargeAirTMP: document.getElementById('DA-T'),
                SpaceTemp: document.getElementById('Space-Temp'),
                DPRCMD: document.getElementById('DPR-cmd'),
                DPRPOS: document.getElementById('DPR-Pos'),
                VAVDIFFPRESS: document.getElementById('VAV-DIFF'),
                AirFlow: document.getElementById('Air-Flow'),
                AirFlowSP: document.getElementById('Air-FlowSP'),
                OutsideAir: document.getElementById('OA-T'),
                ATIVEHEATSP: document.getElementById('ACHEAT-SP'),
                ATIVECOOLSP: document.getElementById('ACCOOL-SP'),
                UnoccupiedCoolSP: document.getElementById('UnOcc-CoolSP'),
                OccupiedCoolSP: document.getElementById('Occ-CoolSP'),
                OccupiedHeatSP: document.getElementById('Occ-HeatSP'),
                UnoccupiedHeatSP: document.getElementById('UnOcc-HeatSP'),


            };

            
                
        // Create subscriber for data changes
        const subscriber = new baja.Subscriber();
        


// Electric Re-Heat
        function updateHeatingGraphic(pointValue) {
            const HeatingImg = document.getElementById('Heating-Coils');
            if (!HeatingImg) return; // Exit if element doesn't exist
            
            let imageName;
            if (pointValue > 0) imageName = 'Coil-Electric-100.svg';
            else{imageName = 'Coil-Electric-OFF.svg';
            }
            HeatingImg.src = '/file/Graphics/TEST/' + imageName;
        }
   
   
// DAMPER
        function updateDamperGraphic(pointValue) {
            const DamperImg = document.getElementById('Damper');
            if (!DamperImg) return; // Exit if element doesn't exist
            
            let imageName;
            if (pointValue > 85.0) imageName = 'RoundDamper10.svg';
            else if (pointValue > 66.6) imageName = 'RoundDamper6.svg';
            else if (pointValue > 50.0) imageName = 'RoundDamper4.svg';
            else if (pointValue > 33.3) imageName = 'RoundDamper2.svg';
            else if (pointValue > 16.6) imageName = 'RoundDamper0.svg';
            else imageName = 'RoundDamper0.svg';
            DamperImg.src = '/file/Graphics/TEST/' + imageName;
        }
   
      
      
        // Subscriber event handler
        subscriber.attach('changed', function(prop) {
            if (prop.getName() === 'out') {
                const pointInfo = this.userData;
                if (pointInfo && elements[pointInfo.elementId]) {
                    const out = this.getOut();
                    const displayValue = out.getValueDisplay();
                    const value = out.getValue();
                    // Check if point is in override mode
                    const isOverridden = this.getStatus().isOverridden();
                    
                    
                    
                    // Special handling for Damper_CMD to update image
                    if (pointInfo.elementId === 'Reheat1') {
                        updateHeatingGraphic(value);
                    }
                    
                    // Special handling for Damper_CMD to update image
                    if (pointInfo.elementId === 'DPRCMD') {
                        updateDamperGraphic(value);
                    }
                    
                   
                    
                    // Update the text element if it exists
                    if (elements[pointInfo.elementId] && elements[pointInfo.elementId].tagName !== 'IMG') {
                        elements[pointInfo.elementId].textContent = displayValue;
                        
                        // Handle override detection for all points
                        const statusElement = elements[pointInfo.elementId];
                        
                        if (isOverridden) {
                            // Point is overridden - show purple
                            statusElement.className = 'data-value override-active';
                            statusElement.style.color = '#8B5CF6'; // Purple color
                            statusElement.textContent = `${displayValue} (Override)`;
                        } else if (value && (value.toString().toLowerCase().includes('on') || value > 0)) {
                            statusElement.className = 'data-value status-on';
                            statusElement.style.color = ''; // Reset to default
                            statusElement.textContent = displayValue;
                        } else {
                            statusElement.className = 'data-value status-off';
                            statusElement.style.color = ''; // Reset to default
                            statusElement.textContent = displayValue;
                        }
                    }
                }
            }
        });
        
        // Function to subscribe to a point
        function subscribeToPoint(pointOrd, elementId) {
            baja.Ord.make(pointOrd).get({ subscriber: subscriber })
                .then(point => {
                    console.log(`Successfully connected to ${elementId}`);
                    
                    // Store reference for the subscriber callback
                    point.userData = { elementId: elementId };
                    
                    // Initial update
                    const out = point.getOut();
                    const displayValue = out.getValueDisplay();
                    const value = out.getValue();
                    const isOverridden = point.getStatus().isOverridden();
                    
                    
                    // Special handling for Damper_CMD to update image initially
                    if (elementId === 'DPRCMD') {
                        updateDamperGraphic(value);
                    }
                 
                    // Special handling for Damper_CMD to update image initially
                    if (elementId === 'Reheat1') {
                        updateHeatingGraphic(value);
                    }
                 
                    
                    // Update text elements
                    if (elements[elementId] && elements[elementId].tagName !== 'IMG') {
                        elements[elementId].textContent = displayValue;
                        
                        // Handle override detection for all points
                        const statusElement = elements[elementId];
                        
                        if (isOverridden) {
                            // Point is overridden - show purple
                            statusElement.className = 'data-value override-on';
                            statusElement.style.color = '#8B5CF6'; // Purple color
                            statusElement.textContent = `${displayValue} (Override)`;
                        } else if (value && (value.toString().toLowerCase().includes('on') || value > 0)) {
                            statusElement.className = 'data-value status-on';
                            statusElement.style.color = ''; // Reset to default
                            statusElement.textContent = displayValue;
                        } else {
                            statusElement.className = 'data-value status-off';
                            statusElement.style.color = ''; // Reset to default
                            statusElement.textContent = displayValue;
                        }
                    }
                })
                .catch(error => {
                    console.error(`Failed to connect to ${elementId}:`, error);
                    if (elements[elementId] && elements[elementId].tagName !== 'IMG') {
                        elements[elementId].textContent = 'Error';
                        elements[elementId].style.color = '#ff6b6b';
                    }
                });
        }
        
        // Subscribe to all data points dynamically
        for (const [pointName, pointOrd] of Object.entries(dataPoints)) {
            subscribeToPoint(pointOrd, pointName);
        }

        // Cleanup on page unload
        window.addEventListener('beforeunload', function() {
            if (subscriber) {
                subscriber.unsubscribeAll();
            }
        });
    });
</script>
    
    
    


    <!-- Light/Dark Mode Toggle Script -->
    <script>
        const toggleSwitch = document.getElementById('toggleSwitch');
        const body = document.body;
        
        // Check for saved theme preference or default to dark mode
        const currentTheme = localStorage.getItem('theme') || 'dark';
        
        if (currentTheme === 'light') {
            body.classList.add('light-mode');
            toggleSwitch.classList.add('active');
        }
        
        toggleSwitch.addEventListener('click', function() {
            body.classList.toggle('light-mode');
            toggleSwitch.classList.toggle('active');
            
            // Save theme preference
            const theme = body.classList.contains('light-mode') ? 'light' : 'dark';
            localStorage.setItem('theme', theme);
        });
    </script>
    
    
    
      <!--Time Tracking-->
      <script>
    function updateDateTime() {
      const now = new Date();
  
      // Format: 30-Jun-2025 2:20 AM CDT (adjust as needed)
      const options = {
        day: '2-digit',
        month: 'short',
        year: 'numeric',
        hour: 'numeric',
        minute: '2-digit',
        hour12: true,
        timeZoneName: 'short'  // Shows CDT, CST, etc.
      };
  
      const formatted = now.toLocaleString('en-US', options).replace(',', '');
      document.getElementById('currentDateTime').textContent = formatted;
    }
  
    // Initial call + update every second
    updateDateTime();
    setInterval(updateDateTime, 1000);
  </script>

<script>
  document.addEventListener("DOMContentLoaded", function () {
    // Handle all toggle-status elements
    const toggles = document.querySelectorAll(".bubbleSP .toggle-status");

    toggles.forEach(toggle => {
      toggle.addEventListener("click", function () {
        // Find the closest .bubbleSP container
        const container = this.closest(".bubbleSP");
        const section = container.querySelector(".override-section");

        // Toggle visibility
        section.style.display = section.style.display === "none" ? "block" : "none";
      });
    });
  });
</script>


